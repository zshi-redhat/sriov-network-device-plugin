{
	"name": "multus-cni-network",
	"type": "multus",
	"delegates": [
		{
			"type": "flannel",
			"name": "flannel-cni",
			"delegate": {
				"isDefaultGateway": true
			}
		},
		{
			"type": "sriov",
			"name": "sriov-cni",
			"if0": "eno1",
			"ipam": {
				"type": "host-local",
				"name": "host-local-cni",
				"subnet": "10.56.217.0/24",
				"rangeStart": "10.56.217.100",
				"rangeEnd": "10.56.217.254",
				"routes": [{
					"dst": "0.0.0.0/0"
				}],
				"gateway": "10.56.217.1"
			},
			"args": {
				"kubeconfig": "/etc/kubernetes/admin.conf"
			}

		}
	],
	"kubeconfig": "/etc/kubernetes/admin.conf"
}
